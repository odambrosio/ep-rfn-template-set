                               displayPager="true"
                               j:bindedComponent="/modules/ep-rfn-template-set/templates/base/generic/news-list/content-area/news-area"
                               jcr:primaryType="jnt:pager"
                               nbOfPages="50"
                               pageSize="10"/>
                                </content-area>
                            </news-list>
                            <section j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Section"/>
                                <content-area jcr:primaryType="jnt:contentList">
                                    <section-area j:allowedTypes="rfnnt:epRfnSubject" jcr:primaryType="jnt:area"/>
                                </content-area>
                            </section>
                            <subject j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Subject"/>
                                <content-area jcr:primaryType="jnt:contentList">
                                    <subject-area j:allowedTypes="jnt:portletReference rfnnt:epRfnRubric rfnnt:epRfnSection rfnnt:epRfnSubject rfnnt:epRfnTheme"
                                      jcr:primaryType="jnt:area"/>

                                                j:numberOfItems="1"
                                                j:skin="skins.custom"
                                                jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                                jcr:primaryType="jnt:absoluteArea"/>
                                    </row-content2-col1>
                                    <row-content2-col0 jcr:primaryType="jnt:contentList">
                                        <left-menu depth="1"
                                      jcr:primaryType="rfnnt:epRfnMenu"
                                      startNode="/modules/ep-rfn-template-set/home"/>
                                    </row-content2-col0>
                                </row-content2>
                            </content-area>
                            <rubric j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Rubric"/>
                                <header-image-area jcr:primaryType="jnt:contentList">
                                    <header-rubric-img-area j:allowedTypes="jnt:imageReference"
                                                j:numberOfItems="1"
                                                jcr:primaryType="jnt:absoluteArea"/>
                                </header-image-area>
                                <content-area jcr:primaryType="jnt:contentList">
                                    <rubric-area j:allowedTypes="rfnnt:epRfnSection rfnnt:epRfnTheme"
                                     jcr:primaryType="jnt:area"/>
                                </content-area>
                            </rubric>
                            <sitemap j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Sitemap"/>
                            </sitemap>
                            <event-list j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Event List"/>
                                <content-area jcr:primaryType="jnt:contentList">
                                    <event-area j:allowedTypes="rfnnt:epRfnEvent" jcr:primaryType="jnt:area"/>
                                    <facets-list j:bindedComponent="/modules/ep-rfn-template-set/templates/base/generic/event-list/content-area/event-area"
                                     j:type="rfnnt:epRfnEvent"
                                     jcr:primaryType="jnt:facets">
                                        <upcomingEvents facet="upcomingEvents"
                                           jcr:primaryType="jnt:queryFacet"
                                           mincount="1"
                                           query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND]">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                label="All upcoming events"
                                                valueLabel="All upcoming events"/>
                                        </upcomingEvents>
                                        <todayEvents facet="todayEvents"
                                        jcr:primaryType="jnt:queryFacet"
                                        mincount="1"
                                        query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND]">
                                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Today"/>
                                        </todayEvents>
                                        <tomorrowEvents facet="tomorrowEvents"
                                           jcr:primaryType="jnt:queryFacet"
                                           mincount="1"
                                           query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND]">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                label="Tomorrow"/>
                                        </tomorrowEvents>
                                        <thisWeekEvents facet="thisWeekEvents"
                                           jcr:primaryType="jnt:queryFacet"
                                           mincount="1"
                                           query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND]">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                label="This week"/>
                                        </thisWeekEvents>
                                        <nextWeekEvents facet="nextWeekEvents"
                                           jcr:primaryType="jnt:queryFacet"
                                           mincount="1"
                                           query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND]">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                label="Next week"/>
                                        </nextWeekEvents>
                                        <next31daysEvents facet="next31daysEvents"
                                             jcr:primaryType="jnt:queryFacet"
                                             mincount="1"
                                             query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND]">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                label="Next 31 days"/>
                                        </next31daysEvents>
                                    </facets-list>
                                </content-area>
                            </event-list>
